{
  "name": "hello-webpack",
  "version": "1.0.0",
  "license": "MIT",
  "author": "chenc",
  "scripts": {
    "dev": "cross-env NODE_ENV=deveploment webpack-dev-server  --config config/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.conf.js",
    "analyzer": "cross-env ANALYZER=true webpack --config config/webpack.prod.conf.js",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "lint-staged": "lint-staged",
    "lint-staged:tsx": "tsc --noEmit && eslint --fix 'src/**/**/*.{js,ts,tsx}'",
    "prettier": "prettier --write src/**/**/*.tsx"
  },
  "browserslist": [
    "cover 99.5% in CN"
  ],
  "dependencies": {
    "antd": "^3.19.8",
    "core-js": "3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@types/compression-webpack-plugin": "^2.0.1",
    "@types/extract-text-webpack-plugin": "^3.0.4",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/node": "^12.7.1",
    "@types/react": "^16.7.21",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.32.1",
    "@types/webpack-bundle-analyzer": "^2.13.2",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^9.2.1",
    "postcss-adaptive": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-hot-loader": "^4.12.10",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "src/**/**/*.tsx": [
      "prettier --write",
      "git add"
    ],
    "**/**/*.tsx": [
      "prettier --write",
      "git add"
    ],
    "**/*.tsx": "npm run lint-staged:tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
